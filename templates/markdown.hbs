{{> header }}
<div class="container">
    <div class="row">
        <div class="nine columns">
            {{{body}}}
        </div>
        <div class="three columns">
            <div class="sidebar">
                <p>
                    <div class="linkbox">
                        <p>
                            <strong>Within this document:</strong>
                        </p>
                        {{{doclinks}}}
                    </div>
                </p>
                <p>
                    {{#if (gt peers_len 0)}}
                    <div class="linkbox">
                        <p>
                            <strong>Other documents:</strong>
                        </p>
                        {{{peers}}}
                    </div>
                    {{/if}}
                </p>
            </div>
        </div>
    </div>
</div>
{{> footer }}
